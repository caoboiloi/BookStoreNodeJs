<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/apple-icon.png">
  <link rel="icon" type="image/png" href="../assets/img/favicon.png">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <title>
    Material Dashboard by Creative Tim
  </title>
  <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
  <%- include ../component/admin/cdn.ejs %>
</head>

<body class="">
  <div class="wrapper ">
    <%- include ../component/admin/sidebar.ejs %>
    <div class="main-panel">
      <!-- Navbar -->
      <%- include ../component/admin/narbar.ejs %>
      <!-- End Navbar -->
      <div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header card-header-primary">
                  <h4 class="card-title ">Danh sách đơn hàng</h4>
                  <p class="card-category"> đơn hàng chưa xác thực & đơn hàng hoàn thành</p>
                </div>
                <div class="card-body">
                  <div class="table-responsive">
                    <table class="table">
                      <thead class=" text-primary">
                        <th>
                          ID đơn hàng
                        </th>
                        <th>
                          ID khách hàng
                        </th>
                        <th>
                          Tên khách hàng
                        </th>
                        <th>
                          Ngày đặt hàng
                        </th>
                        <th>
                          Ngày giao dự kiến
                        </th>
                        <th>
                          Nhà vận chuyển
                        </th>
                        <th>
                          Phí giao hàng
                        </th>
                        <th>
                          Tổng tiền hàng
                        </th>
                        <th>
                          Tổng hoá đơn
                        </th>
                        <th colspan="2">
                          Tình trạng
                        </th>
                      </thead>
                      <tbody>
                        <% buys.forEach(b => { %>
                          <tr>
                            <td>
                              <a href="/admin/buys?id=<%= b._id %>"><%= b._id.toString().substring(0,8) %>...</a>
                            </td>
                            <td>
                              <a href="/admin/customer?id=<%= b.user._id %>"><%= b.user._id.toString().substring(0,8) %>...</a>
                            </td>
                            <td>
                              <%= b.user.name %>
                            </td>
                            <td>
                              <%= moment(b.orderDate).format('HH:mm DD/MM/YYYY') %>
                            </td>
                            <td>
                              <%= moment(b.deliveryDate).format('HH:mm DD/MM/YYYY') %>
                            </td>
                            <td>
                              <%= b.shipper.item.name %>
                            </td>
                            <td class="text-primary">
                              <%= b.shipper.price %>
                            </td>
                            <td class="text-primary">
                              <%= b.totalPrice.toLocaleString() %>
                            </td>
                            <td class="text-primary">
                              <%= (b.totalPrice + b.shipper.price).toLocaleString() %>
                            </td>
                            <% if (b.validation) { %>
                              <td>
                                Đã hoàn thành
                              </td>
                              <td>

                              </td>
                            <% } else { %>
                              <td>
                                Đang xác thực
                              </td>
                              <td>
                                <button type="button" rel="tooltip" title="Xác thực" class="btn btn-primary btn-link btn-sm">
                                  <i class="material-icons">check</i>
                                </button>
                              </td>
                            <% } %>
                          </tr>
                        <% }) %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
